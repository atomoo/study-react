{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "member-access": false,
      "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
      "interface-name": false,
      "quotemark": [true, "single", "jsx-double"],
      "arrow-parens": false,
      "one-line": [true, "check-open-brace"],
      "no-console": false
    },
    "rulesDirectory": []
}